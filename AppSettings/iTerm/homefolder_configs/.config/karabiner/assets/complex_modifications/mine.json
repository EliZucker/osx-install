{
	"title": "Fn + Backspace to Backslash, Backslash to Backspace",
	"rules": [{
			"description": "Map Fn + Backspace to Forward Delete",
			"manipulators": [{
				"type": "basic",
				"from": {
					"key_code": "delete_or_backspace",
					"modifiers": {
						"mandatory": [
							"fn"
						]
					}
				},
				"to": [{
					"key_code": "backslash"
				}]
			}]
		},
		{
			"description": "Map Backslash to Backspace",
			"manipulators": [{
				"type": "basic",
				"from": {
					"key_code": "backslash"
				},
				"to": [{
					"key_code": "delete_or_backspace"
				}]
			}]
		}
	]
}
